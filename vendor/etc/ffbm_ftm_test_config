tc 1
#HeadSet Mic to  Speakers AFE loopback   open spk
!AfeLoop
gkv_rx:PCM_RX_LOOPBACK-SPEAKER
be_name_rx:MI2S-LPAIF-RX-PRIMARY
pcm_id_rx:100
channels_rx:2
gkv_tx:0-HEADPHONE_MIC
be_name_tx:CODEC_DMA-LPAIF_RXTX-TX-3
pcm_id_tx:101
channels_tx:1

enable
TX DEC0 MUX:SWR_MIC
TX SMIC MUX0:SWR_MIC5
TX_AIF1_CAP Mixer DEC0:1
ADC2 MUX:INP2
ADC2 ChMap:SWRM_TX2_CH2
TX1 MODE:ADC_LP
HDR12 MUX:NO_HDR12
ADC2_MIXER Switch:1
AMIC2_MIXER Switch:1
ADC2 Volume:10
TX_DEC0 Volume:86
TFA_CHIP_SELECTOR:1
TFA Profile:speaker


