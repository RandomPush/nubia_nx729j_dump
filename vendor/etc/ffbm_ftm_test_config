tc 3
# AMIC1 to Headset Speakers AFE loopback   open mic1
!AfeLoop
gkv_rx:PCM_RX_LOOPBACK-HEADPHONES
be_name_rx:CODEC_DMA-LPAIF_RXTX-RX-0
pcm_id_rx:100
channels_rx:2
gkv_tx:0-HANDSETMIC
be_name_tx:CODEC_DMA-LPAIF_RXTX-TX-3
pcm_id_tx:101
channels_tx:1

enable
RX_MACRO RX1 MUX:AIF1_PB
RX_MACRO RX0 MUX:AIF1_PB
RX INT0_1 MIX1 INP0:RX0
RX INT1_1 MIX1 INP0:RX1
RX INT0 DEM MUX:CLSH_DSM_OUT
RX INT1 DEM MUX:CLSH_DSM_OUT
RX_COMP1 Switch:1
RX_COMP2 Switch:1
HPHL_RDAC Switch:1
HPHR_RDAC Switch:1
RX_RX0 Digital Volume:80
RX_RX1 Digital Volume:80
HPHL Volume:4
HPHR Volume:4

TX DEC2 MUX:SWR_MIC
TX SMIC MUX2:SWR_MIC4
TX_AIF1_CAP Mixer DEC2:1
ADC1 ChMap:SWRM_TX2_CH1
TX0 MODE:ADC_LP
ADC1_MIXER Switch:1
AMIC1_MIXER Switch:1
ADC1 Volume:1

tc 33
